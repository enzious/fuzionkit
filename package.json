{
  "name": "fuzionkit",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "files": [
    "*"
  ],
  "exports": {
    "./*.js": "./lib/*.js",
    "./*": "./lib/*/index.js",
    "./*/index.js": null
  },
  "watch": {
    "build:js": {
      "patterns": ".",
      "extensions": "ts",
      "ignore": [
        "*.d.ts"
      ]
    },
    "build:css": {
      "patterns": ".",
      "extensions": "scss"
    }
  },
  "scripts": {
    "build": "run-s clean:dist build:css build:js",
    "clean:dist": "node scripts/clean-dist.js",
    "build:js": "tsc || exit 1",
    "build:css": "node scripts/build-css.js || exit 1",
    "lint": "eslint .",
    "lint:debug": "eslint --debug .",
    "lint:fix": "eslint --fix .",
    "postinstall": "run-s build",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git@lab.braindead.io:fuzionkit/fuzionkit.git"
  },
  "author": "enzi (enzi@braindead.io)",
  "license": "ISC",
